server.port=80
debug=false
#一般情况下，小项目通常都是在Tomcat下部署多个webapp，通过上下文区分
#集群或者中大型项目，通常一个Tomcat部署一个webapp，通过端口号区分
server.servlet.context-path=/

spring.http.encoding.charset=UTF-8
#开启thymeleaf缓存
#除了设置这个属性外
#还需要设置file->settings->build...->complier->build project auto...
spring.thymeleaf.cache=false

spring.mvc.date-format=yyyy-MM-dd
#JSON日期序列化输出
spring.jackson.time-zone=GMT+9
spring.jackson.date-format=yyyy-MM-dd HH:mm:ss SSS

#日志路径
#logging.file=E:/idea-workspace/logs/spring-boot-sample/error.log
#日志级别 debug->info->warn->error
#root代表全局设置
#logging.level.root=error
#设置指定包输出
#logging.level.org.springframework=ERROR